<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" android:id="@+id/rootLayout">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="BEE"
        android:textSize="30dp"
        android:typeface="monospace"

        android:id="@+id/movieName"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />


    <FrameLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/stick_container"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal"
        android:tag="top_container"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="-30dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/movieName"
        android:layout_alignParentEnd="true">

        <RelativeLayout
            android:id="@+id/adam_flayman"
            android:layout_width="118dp"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:orientation="vertical"

            android:weightSum="1"
            android:layout_gravity="left|top"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_below="@+id/movieName"
            android:layout_alignStart="@+id/stick_container"
            android:layout_marginTop="15dp">

            <ImageView
                android:id="@+id/adam_flayman_pic"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:src="@drawable/bee_adam_flayman_bg"
                android:layout_alignParentEnd="true"
                android:layout_alignParentStart="true" />


            <TextView
                android:id="@+id/adam_flayman_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="#9"
                android:textSize= "25dp"
                android:textStyle="bold"
                android:textColor="#ffffff"
                android:layout_marginLeft="1dp"
                android:layout_marginRight="1dp"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/berry_b_benson"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:gravity="right"
            android:orientation="vertical"
            android:weightSum="1"
            android:layout_gravity="left|center_vertical"
            android:layout_below="@+id/janet"
            android:layout_toStartOf="@+id/movieName"
            android:layout_marginTop="30dp">



            <ImageView
                android:id="@+id/berry_b_benson_img"
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:src="@drawable/bee_berry_b_benson_burned_bg"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentStart="true" />


            <TextView
                android:id="@+id/berry_b_benson_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="#10"
                android:textSize= "25dp"
                android:textStyle="bold"
                android:textColor="#ffffff"
                android:layout_marginLeft="1dp"
                android:layout_marginRight="1dp"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/janetContainer"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:orientation="vertical"
            android:layout_gravity="right|bottom"

            android:layout_alignTop="@+id/container_4"
            android:layout_toStartOf="@+id/movieName"
            android:layout_marginBottom="70dp"
            android:layout_marginRight="40dp">

            <ImageView
                android:id="@+id/janet_img"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:src="@drawable/bee_janet_benson_bg"
                android:layout_alignParentEnd="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentStart="true" />


            <TextView
                android:id="@+id/janet_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/janet_img"
                android:layout_alignTop="@+id/janet_img"
                android:layout_alignRight="@+id/janet_img"
                android:layout_alignBottom="@+id/janet_img"
                android:gravity="center"
                android:text="#11"
                android:textSize= "25dp"
                android:textStyle="bold"
                android:textColor="#ffffff"
                android:layout_alignParentEnd="false"
                android:layout_alignParentStart="false"
                android:layout_alignParentLeft="false"
                android:layout_alignParentTop="false"
                android:layout_alignParentRight="false"
                android:layout_marginLeft="0dp"
                android:layout_marginRight="20dp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/larry_king"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:gravity="right"
            android:orientation="vertical"
            android:weightSum="1"
            android:layout_gravity="right|center_vertical"
            android:layout_below="@+id/mooseblood"
            android:layout_alignParentEnd="true">



            <ImageView
                android:id="@+id/larry_img"
                android:layout_width="160dp"
                android:layout_height="160dp"
                android:src="@drawable/bee_larry_king_bg"
                android:layout_alignParentEnd="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentStart="true" />


            <TextView
                android:id="@+id/larry_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="#12"
                android:textSize= "25dp"
                android:textStyle="bold"
                android:textColor="#ffffff"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="1dp"
                android:layout_marginTop="40dp"
                android:layout_alignParentStart="true"
                android:layout_alignBottom="@+id/sticker_8"
                android:layout_alignParentEnd="true" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/layton"
            android:layout_width="140dp"
            android:layout_height="128dp"
            android:gravity="right"
            android:orientation="vertical"
            android:weightSum="1"
            android:layout_gravity="top|center_horizontal"
            android:layout_below="@+id/mooseblood"
            android:layout_alignStart="@+id/mooseblood"
            android:layout_alignBottom="@+id/container_3"
            android:layout_alignEnd="@+id/mooseblood"
            android:layout_marginRight="10dp">



            <ImageView
                android:id="@+id/layton_img"
                android:layout_width="130dp"
                android:layout_height="130dp"
                android:src="@drawable/bee_layton_t_montgomery_bg"
                android:layout_alignParentEnd="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentStart="true" />


            <TextView
                android:id="@+id/layton_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="#13"
                android:textSize= "25dp"
                android:textStyle="bold"
                android:textColor="#ffffff"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="1dp"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/lou"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:gravity="right"
            android:orientation="vertical"
            android:weightSum="1"
            android:layout_gravity="right|top"
            android:layout_below="@+id/movieName"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="30dp">



            <ImageView
                android:id="@+id/lou_img"
                android:layout_width="130dp"
                android:layout_height="130dp"
                android:src="@drawable/bee_lou_lo_luca_bg"
                android:layout_alignParentEnd="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentStart="true" />


            <TextView
                android:id="@+id/lou_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="#14"
                android:textSize= "25dp"
                android:textStyle="bold"
                android:textColor="#ffffff"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="1dp"
                android:layout_centerVertical="true"
                android:layout_alignParentStart="true" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/mooseblood"
            android:layout_width="170dp"
            android:layout_height="127dp"
            android:gravity="right"
            android:orientation="vertical"
            android:weightSum="1"
            android:layout_gravity="center"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_above="@+id/container_3"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="-40dp">

            <ImageView
                android:id="@+id/mooseblood_img"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/bee_mooseblood_burned_bg"
                android:layout_alignParentEnd="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentStart="true"
                android:layout_alignParentBottom="true" />


            <TextView
                android:id="@+id/mooseblood_txt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="#15"
                android:textSize= "25dp"
                android:textStyle="bold"
                android:textColor="#ffffff"
                android:layout_marginLeft="1dp"
                android:layout_marginRight="1dp"
                android:layout_marginTop="32dp"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true" />
        </RelativeLayout>



        <ImageView
            android:id="@+id/berry_b_benson_o"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_margin="5dp"
            android:layout_gravity="left|bottom" />

        <ImageView
            android:id="@+id/adam_flayman_o"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_margin="5dp"
            android:layout_gravity="left|bottom" />

        <ImageView
            android:id="@+id/layton_o"
            android:layout_width="140dp"
            android:layout_height="140dp"
            android:layout_margin="5dp"
            android:layout_gravity="left|bottom" />

        <ImageView
            android:id="@+id/larry_king_o"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_margin="5dp"
            android:layout_gravity="left|bottom" />

        <ImageView
            android:id="@+id/lou_o"
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:layout_margin="5dp"
            android:layout_gravity="left|bottom" />

        <ImageView
            android:id="@+id/mooseblood_o"
            android:layout_width="170dp"
            android:layout_height="130dp"
            android:layout_margin="5dp"
            android:layout_gravity="left|bottom" />

        <ImageView
            android:id="@+id/janet_o"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_margin="5dp"
            android:layout_gravity="left|bottom" />

    </FrameLayout>
</RelativeLayout>
